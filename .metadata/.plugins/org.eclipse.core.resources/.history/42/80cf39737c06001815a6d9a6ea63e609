package View;

import javax.swing.JFrame;
import javax.swing.SpringLayout;
import javax.swing.JButton;

public class VentanaPrincipalTecnico extends JFrame{
	public VentanaPrincipalTecnico() {
		SpringLayout springLayout = new SpringLayout();
		getContentPane().setLayout(springLayout);
		
		JButton btnAadirEcg = new JButton("AÃ±adir ECG");
		springLayout.putConstraint(SpringLayout.WEST, btnAadirEcg, 43, SpringLayout.WEST, getContentPane());
		springLayout.putConstraint(SpringLayout.SOUTH, btnAadirEcg, -121, SpringLayout.SOUTH, getContentPane());
		getContentPane().add(btnAadirEcg);
	}
}
